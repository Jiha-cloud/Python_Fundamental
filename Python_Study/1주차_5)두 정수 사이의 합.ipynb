    "def solution(a, b):\n",
    "    answer=0\n",
    "    if a > b:\n",
    "        for i in range(b,a+1):\n",
    "            answer += i\n",
    "    elif a < b:\n",
    "        for i in range(a,b+1):\n",
    "            answer += i\n",
    "    else:\n",
    "        answer = a\n",
    "    return answer"
   ]
