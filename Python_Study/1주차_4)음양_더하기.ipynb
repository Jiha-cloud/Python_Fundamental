    "def solution(absolutes, signs):\n",
    "    answer = 0\n",
    "    for i in range(0,len(signs)):\n",
    "        if signs[i]==False:\n",
    "            answer -= absolutes[i]\n",
    "        else:\n",
    "            answer += absolutes[i]\n",
    "        \n",
    "    return answer\n",
    "\n",
    "print(solution([1,2,3],[True,False,True]))"
   ]
